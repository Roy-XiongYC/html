<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<!-- blog -->
	<bean id="blogUserRepository"
		class="com.person.web.blog.repository.impl.BlogUserRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogLinkRepository"
		class="com.person.web.blog.repository.impl.BlogLinkRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogAboutRepository"
		class="com.person.web.blog.repository.impl.BlogAboutRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogShuoRepository"
		class="com.person.web.blog.repository.impl.BlogShuoRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogRijiRepository"
		class="com.person.web.blog.repository.impl.BlogRijiRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogLearnRepository"
		class="com.person.web.blog.repository.impl.BlogLearnRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogGuestRepository"
		class="com.person.web.blog.repository.impl.BlogGuestRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="blogXcRepository"
		class="com.person.web.blog.repository.impl.BlogXcRepositoryImpl"
		parent="abstractRepository"></bean>

	<!-- blog -->

	<!-- fs -->
	<bean id="filesRepositoryImpl" class="com.person.web.fs.repository.impl.FilesRepositoryImpl" parent="abstractRepository"/>
	<bean id="fileSettingRepositoryImpl" class="com.person.web.fs.repository.impl.FileSettingRepositoryImpl" parent="abstractRepository"/>
	<!-- fs -->

	<!-- sys -->
	<bean id="sequenceRepository"
		class="com.person.web.blog.repository.impl.SequenceRespositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysGroupResourceRepository"
		class="com.person.web.admin.repository.impl.SysGroupResourceRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysResourceRepository"
		class="com.person.web.admin.repository.impl.SysResourceRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysGroupUserRepository"
		class="com.person.web.admin.repository.impl.SysGroupUserRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysGroupRepository"
		class="com.person.web.admin.repository.impl.SysGroupRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysUserRepository"
		class="com.person.web.admin.repository.impl.SysUserRepositoryImpl"
		parent="abstractRepository"></bean>
	<bean id="sysDictRepository"
		class="com.person.web.admin.repository.impl.SysDictRepositoryImpl"
		parent="abstractRepository"></bean>
	<!-- sys -->
	
	<!-- school -->
	<bean id="schoolScoreRepository" class="com.person.web.admin.repository.impl.SchoolScoreRepositoryImpl" parent="abstractRepository"></bean>
	<bean id="schoolStudentRepository" class="com.person.web.admin.repository.impl.SchoolStudentRepositoryImpl" parent="abstractRepository"></bean>
	<bean id="schoolProjectRepository" class="com.person.web.admin.repository.impl.SchoolProjectRepositoryImpl" parent="abstractRepository"></bean>
	
</beans>